<%- include("partials/header") -%>
    <h1>Compose</h1>

    <form class="" action="/compose" method="post">
      <div class="form-group">
        <label>Title</label>
        <input class="form-control" type="text" name="titleText">
      </div>
      <div class="form-group">
        <label>Post</label>
        <textarea class="form-control" name="postText" rows="8" cols="80"></textarea>
      </div>
      <button class="btn btn-primary" type="submit" name="publish">Publish</button>
    </form>
<%- include("partials/footer") -%>
